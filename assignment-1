if n % 2 == 0:
    print("Even")
else:
    print("Odd")


#question.3
    year = int(input())

if (year % 400 == 0) or (year % 4 == 0 and year % 100 != 0):
    print("Leap Year")
else:
    print("Not Leap Year")


